    "autoload": {
        "psr-4": {
            "lucianserpi\\exampleSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lucian Serpi"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": "^7.2.5",
      "josantonius/request": "^2.0",
      "mjaschen/phpgeo": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
